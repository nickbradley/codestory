{
  "manifest_version": 2,
  "name": "CodeStory",
  "description": "Improves code reviews and maintenance",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["http://stackoverflow.com/*", "https://github.com/*"],
      "js": ["md5.js", "jquery.js", "codestory.js", "chrome-init.js"]
    }
  ]
}